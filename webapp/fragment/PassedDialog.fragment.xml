<core:FragmentDefinition xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog showHeader="true" title="You Passed!" draggable="false" escapeHandler="passedDialogEscapeHandler" class="passedDialog">
		<subHeader>
			<Bar>
				<contentMiddle>
					<Title id="passedDialogScoreText" text=""/>
				</contentMiddle>
			</Bar>
		</subHeader>
		<l:Grid defaultSpan="L12 M12 S12">
			<l:content>
				<HBox justifyContent="Center">
					<Button text="Play Next Movie" iconFirst="false" press="onPressPlayNextMovie" class="homePageCenterButtons menuButtons"/>
				</HBox>
				<HBox justifyContent="Center">
					<Button text="Go To Levels" iconFirst="false" press="onPressGoToLevels" class="homePageCenterButtons menuButtons"/>
				</HBox>
			</l:content>
		</l:Grid>
	</Dialog>
</core:FragmentDefinition>